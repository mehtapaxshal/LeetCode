class Solution {
public:
    int reverse(int x) {
        
        int m,result=0;
        vector<int> r;
        
        while(x!=0)
        {
            m=x%10;
            x=x/10;
            r.push_back(m);
        }
        
        for(int i=r.size()-1;i>=0;i--)
        {
            result+=r[i]*(pow(10,r.size()-1-i));
            
        }
        
        if(result<=INT_MIN || result>=INT_MAX)
            return 0;

        return result;
    }
};
