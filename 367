class Solution {
public:
    bool isPerfectSquare(int num) {
        
        if(num==1)
            return true;
        
        for(int i=1;i<=num;i=i+2)
        {
            num=num-i;
            if(num==0)
            {
                
                return true;
            }
        }
        
        return false;
    }
};
